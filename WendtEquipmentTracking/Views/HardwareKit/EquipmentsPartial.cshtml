@model IEnumerable<EquipmentModel>

<h4>Hardware</h4>

<table class="table table-condensed table-bordered">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EquipmentName)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.EquipmentName)
                </td>
            </tr>
        }
    </tbody>
</table>
